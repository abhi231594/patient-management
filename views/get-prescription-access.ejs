<% if(prescriptions.length){ %>
  <table class="mdl-data-table mdl-js-data-table mdl-shadow--2dp">
    <thead>
      <tr>
        <th class="mdl-data-table__cell--non-numeric">Sr.no</th>
        <th class="mdl-data-table__cell--non-numeric">Patient Name</th>
        <th class="mdl-data-table__cell--non-numeric">Prescription Prescribed By</th>
        <th>Date</th>
        <th>Check details</th>
      </tr>
    </thead>
    <tbody>
       <% for(var i=0; i < prescriptions.length; i++) { %>
        <tr>
          <td><%=i+1%></td>
          <td class="mdl-data-table__cell--non-numeric"><%=prescriptions[i].prescribedForId.name%></td>
          <td class="mdl-data-table__cell--non-numeric"><%=prescriptions[i].prescribedById.name%></td>
          <td class="mdl-data-table__cell--non-numeric"><%=prescriptions[i].createdAt%></td>
          <td class="mdl-data-table__cell--non-numeric">
            <a class="mdl-button mdl-button--colored mdl-js-button mdl-js-ripple-effect" href="/prescription-records/<%=prescriptions[i].id%>" target="_blank">
              click here
            </a>
          </td>
        </tr>
      <% } %>
    </tbody>
  </table>
 <% } else { %>
   <h6>No Prescription Record Found</h6>
<% } %>
